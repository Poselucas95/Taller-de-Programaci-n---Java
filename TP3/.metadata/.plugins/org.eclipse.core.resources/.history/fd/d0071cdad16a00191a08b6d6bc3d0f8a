package ar.edu.ort.tp3;

public class NumeroTelefonico {
	private int caracteristica;
	private int nroDeAbonado;
	private int codigoPais;
	private TipoDeLinea tipo;
	
	public NumeroTelefonico(int caracteristica, int nroDeAbonado, int codigoPais, TipoDeLinea tipo){
		this.caracteristica = caracteristica;
		this.nroDeAbonado = nroDeAbonado;
		this.codigoPais = codigoPais;
		this.TipoDeLinea = tipo;
	}
}
